{##
 # Wholesale prices table
 #
 # @ListChild (list="wholesale.price", weight="20")
 #}

{% if this.isWholesalePricesEnabled() %}
<table class="wholesale-prices-product-block">

  {% for wholesalePrice in this.getWholesalePrices() %}
    <tr class="price-row">
  
      {{ widget_list('widgetlist', type='nested', wholesalePrice=wholesalePrice) }}
  
    </tr>
  {% endfor %}

</table>
{% endif %}
