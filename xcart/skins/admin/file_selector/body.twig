{% form '\\XLite\\View\\Form\\FileDialog\\Select' with {formParams: this.getFileDialogParams()} %}
<div class="file-selector">
  <ul class="selections-list">
    {% if this.isAllMode() %}
      {{ widget_list('file_select_dialog.file_selections') }}
    {% elseif this.isUrlMode() %}
      {{ widget_list('file_select_dialog.file_selections_url') }}
    {% endif %}
  </ul>
{{ widget_list('file_select_dialog.actions') }}
</div>
{% endform %}

