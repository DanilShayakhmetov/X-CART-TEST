{##
 # Google feed generation progress section
 #}

<div class="generation-progress event-task-progress-page">

  <div class="content">
    {% form 'XLite\\Module\\XC\\GoogleFeed\\View\\Form\\FeedGeneration' with {formAction: 'feedGenerationCancel'} %}
    <div class="subcontent clearfix">
      {{ widget('XLite\\View\\EventTaskProgress', event=this.getEventName(), message=this.getProgressMessage(), showCancel=false ) }}
    </div>
    {{ widget('XLite\\View\\StickyPanel\\EventProgress') }}
    {% endform %}
  </div>

</div>
