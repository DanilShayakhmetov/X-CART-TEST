{
  "name": "xcart/silex-annotations",
  "description": "A silex service provider that allows the use of annotations.",
  "keywords": ["silex", "annotation"],
  "license": "https://www.x-cart.com/license-agreement.html",
  "authors": [
    {
      "name": "Maksim D. Kuznetsov",
      "email": "just.mdk@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|~7.1",
    "silex/silex": "^2.0",
    "doctrine/annotations": "~1.0",
    "doctrine/cache": "^1.6.2",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "~1.0",
    "symfony/browser-kit": "^3.3.13",
    "symfony/security": "^3.3.13"
  },
  "suggest": {
    "symfony/security": "You will need this package to use the `Secure` annotation"
  },
  "autoload": {
    "psr-4": {
      "XCart\\SilexAnnotations\\": "src/XCart/SilexAnnotations"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XCart\\SilexAnnotationsTest\\": "test/XCart/SilexAnnotationsTest"
    }
  },
  "minimum-stability": "stable"
}